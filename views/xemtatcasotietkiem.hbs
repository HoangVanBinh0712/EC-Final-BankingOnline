<div class="w3l-news" id="news">
  <section id="grids5-block" class="py-5">
    <div class="container py-lg-5 py-md-4 py-2">
      <p>{{message}}</p>
      <p>Tổng số lượng sổ: {{soluong}}</p>
      <p>Tổng số tiền được gửi: {{tongtien}}</p>
      <div class="row mt-lg-5 mt-4">
        <p>{{message}}</p>
        {{#each sotietkiem}}
          <div class="col-lg-4 col-md-6 mt-md-0 mt-5">
            <div class="grids5-info">
              <h5 class="card-title">{{this.TenSo}}</h5>
              <p class="card-text"> {{this.STK}}</p>
              <p class="card-text"> {{this.CCCD}}</p>
              <p class="card-text"> {{this.SoTienGui}}</p>
              <p class="card-text"> {{this.ThoiHan}}</p>
              <p class="card-text">{{this.LaiSuat}} </p>
              <p class="card-text"> {{this.NgayGui}}</p>
              <p class="card-text">{{this.NgayHetHan}} </p>
              <p class="card-text">UserId: {{this.user}} </p>
            </div>
          </div>
        {{/each}}
      </div>
    </div>
  </section>
</div>